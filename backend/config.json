{
  "app": "event-analysis",
  "host": "localhost",
  "port": "8080",
  "connection": {
    "sqlite": "event-analysis.db"
  },

  "logger": {
      "version": 1,
      "disable_existing_loggers": true,
      "formatters": {
          "console": {
              "format": "%(asctime)s — [ %(levelname)s ] — %(message)s"
          }
      },
      "handlers": {
          "console": {
              "class": "logging.StreamHandler",
              "stream": "ext://sys.stdout",
              "formatter": "console"
          }
      },
      "loggers": {
          "": {
              "handlers": [
                "console"
              ],
              "level": "INFO",
              "propagate": true
          }
      }
  }
}
